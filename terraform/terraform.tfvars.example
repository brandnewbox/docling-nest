# DigitalOcean Configuration
# Copy this file to terraform.tfvars and fill in your values

# DigitalOcean region (default: nyc1)
# Available regions: nyc1, nyc3, sfo3, ams3, sgp1, lon1, fra1, tor1, blr1, syd1
region = "nyc1"

# Git repository containing your function code
git_repo_url = "https://github.com/your-username/docling-nest"

# Git branch to deploy (default: master)
git_branch = "master"

# Log level for the function (default: INFO)
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
log_level = "INFO"
